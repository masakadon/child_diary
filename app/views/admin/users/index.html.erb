<h1>会員一覧</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>名前</th>
      <th>メールアドレス</th>
      <th>登録日</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= l(user.created_at, format: :short) %></td>
        <td><%= link_to '詳細', admin_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>